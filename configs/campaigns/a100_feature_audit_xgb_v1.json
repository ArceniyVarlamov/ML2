{
  "name": "a100_feature_audit_xgb_v1",
  "description": "Feature audit (OOF permutation + null) on XGB with rarehard pilot and all41 follow-up.",
  "workdir": "../..",
  "log_dir": "artifacts_campaigns/a100_feature_audit_xgb_v1",
  "env": {
    "OMP_NUM_THREADS": "8",
    "MKL_NUM_THREADS": "8",
    "NUMEXPR_NUM_THREADS": "8"
  },
  "tasks": [
    {
      "id": "audit_rarehard_run",
      "cmd": "./.venv/bin/python scripts/feature_audit_oof.py --config configs/audit/feature_audit_xgb_rarehard_v1.json run",
      "done_if_all_exist": [
        "artifacts_feature_audit_xgb_rarehard_v1/summary.json",
        "artifacts_feature_audit_xgb_rarehard_v1/keep_extra.json",
        "artifacts_feature_audit_xgb_rarehard_v1/drop_columns.json"
      ]
    },
    {
      "id": "audit_all41_run",
      "cmd": "./.venv/bin/python scripts/feature_audit_oof.py --config configs/audit/feature_audit_xgb_all41_v1.json run",
      "done_if_all_exist": [
        "artifacts_feature_audit_xgb_all41_v1/summary.json",
        "artifacts_feature_audit_xgb_all41_v1/keep_extra.json",
        "artifacts_feature_audit_xgb_all41_v1/drop_columns.json"
      ]
    }
  ]
}
